# FILE PURPOSE:
# - Template for environment variables
# - Copy this to .env and fill in your Firebase credentials
# - Never commit .env to version control

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Admin Email (for initial admin setup)
VITE_ADMIN_EMAIL=admin@mitc.com

# Image Upload API (optional - for imgbb or Cloudinary)
VITE_IMAGE_UPLOAD_API_KEY=optional_api_key